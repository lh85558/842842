
#!/bin/sh /etc/rc.common
# Print Server initialization script

START=98
STOP=15

USE_PROCD=1

start_service() {
    procd_open_instance
    procd_set_param command /bin/sh -c "while true; do /usr/bin/print-server-monitor; sleep 30; done"
    procd_set_param respawn
    procd_set_param stderr 1
    procd_close_instance
}
